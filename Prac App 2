#Q1
def q1(sentance):
#split string into list of words
Mylist = sentance.split('')
print(Mylist)

# parse through list and append to new list
Newlist = []
for i in range(len(Mylist) - 1,-1, -1):
  #print(i)
  Newlist.append(Mylist[i])
  #print(Newlist)
  
pass
q1('hello world pie test')
