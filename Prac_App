def q3(lst,n):

#print(lst)
#print(type(lst))
#print(n)
#print(type(n))

    return lst[-n::]
    pass
    ######################################                     #Q4
def q4(strng):
  
    #print(str)
    #print(type(strng))
    lst = []
    for i in strng:
        #print(ord(i))
        lst.append(ord(i))
    return lst
    pass
###########################################                  #Q5
    #print(strng)
    #print(type(strng))
    return tuple(strng.split())
    pass
