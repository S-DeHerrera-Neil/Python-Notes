##################################################              #Q9
def q9(miltime):

#print(miltime)
#print(type(miltime))
     if miltime >= 301 and miltime < 1200:
         return 'Good Morning'
     elif miltime >= 1201 and miltime < 1600:
          return 'Good Afternoon'
     elif miltime >= 1600 and miltime < 2059:
          return 'Good Evening'
     else:
          return 'Good Night'
          pass

###########################################                     #Q10
def q10(numlist):
    
    #print(numlist)
    #print(type(numlist))
    for i in numlist:
        if i < 0:
            return False
    else:
    return True
    pass

